# Public Supabase settings consumed by Next.js during build/runtime.
NEXT_PUBLIC_APP_ENV=local
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-public-anon-key

# Optional: override the server-only URL when it differs from the public URL.
SUPABASE_URL=https://your-project-id.supabase.co

# Onboarding checklist + feature flag placeholders so quickstart seeds have values.
NEXT_PUBLIC_ONBOARDING_CHECKLIST_FLAG=onboarding_checklist_v1
NEXT_PUBLIC_ONBOARDING_CHECKLIST_VERSION=1
NEXT_PUBLIC_ONBOARDING_DISCLOSURE_BUCKET=onboarding-disclosures

# Datadog RUM + telemetry settings referenced by onboarding scripts/tests.
NEXT_PUBLIC_DATADOG_SITE=datadoghq.com
NEXT_PUBLIC_DATADOG_CLIENT_TOKEN=your-datadog-client-token
NEXT_PUBLIC_DATADOG_APPLICATION_ID=your-datadog-application-id

# Backend onboarding API base URL (FastAPI service from apps/backend).
ONBOARDING_API_URL=http://localhost:8000

# Optional: provide a workspace UUID that matches the seeded onboarding workspace when your profile metadata is empty.
ONBOARDING_DEFAULT_WORKSPACE_ID=

# Document additional redirect origins in Supabase dashboard:
#   http://localhost:3000/auth/callback
#   https://staging.example.com/auth/callback
#   https://app.example.com/auth/callback
